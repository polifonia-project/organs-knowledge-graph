        rdf:type               ins:InstrumentWhole ;
        rdfs:comment           "De Bourdon 16 Ped is van C-H gecombineerd met die van het Man." ;
        organ:hasPitch         "a1 = 4   Hz" ;
        organ:hasTemperament   "evenredig zwevend" ;
